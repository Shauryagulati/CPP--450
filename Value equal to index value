class Solution{
public:

	vector<int> valueEqualToIndex(int arr[], int n) {
	    // code here
	    int count = 1;
	    vector<int> ans;
	    for(int i=0; i<n; i++) {
            if(arr[i] == count)
            {
                ans.push_back(count);
            }
        count++;
        } return ans;
	}
};
